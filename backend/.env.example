# Azure VM API
HOST=0.0.0.0
PORT=3000
DB_PATH=./data/print_jobs.db
UPLOAD_DIR=./data/uploads
MAX_UPLOAD_BYTES=5242880

# Security
# Required for local print agent
AGENT_TOKEN=change-me-strong-agent-token
# Secret used to sign user access tokens
APP_SECRET=change-me-super-secret
# 12 hours by default
ACCESS_TOKEN_TTL_SECONDS=43200
# Auto-created users at startup, format: user:pass,user2:pass2
DEFAULT_USERS=admin:admin123
